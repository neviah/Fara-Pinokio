# Fara-7B Configuration
# Copy this file to .env and customize as needed

# Model Configuration
FARA_MODEL_ENDPOINT=http://localhost:5000/v1
FARA_API_KEY=not-needed
FARA_MAX_ROUNDS=50
FARA_HEADLESS=true

# BrowserBase Configuration (optional)
# Uncomment and set if using BrowserBase for browser management
# BROWSERBASE_API_KEY=your_api_key_here
# BROWSERBASE_PROJECT_ID=your_project_id_here

# Azure Foundry Configuration (optional)
# Uncomment and set if using Azure Foundry
# AZURE_OPENAI_ENDPOINT=https://your-endpoint.inference.ml.azure.com/
# AZURE_OPENAI_API_KEY=your_key_here

# Gradio Configuration
GRADIO_SERVER_NAME=0.0.0.0
GRADIO_SERVER_PORT=7860
GRADIO_SHARE=false
GRADIO_DEBUG=false

# Logging Configuration
LOG_LEVEL=INFO
ENABLE_SCREENSHOTS=true
DOWNLOADS_FOLDER=./downloads